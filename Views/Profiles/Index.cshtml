@Model IEnumerable<ConstruccionDeSoftware_ProyectoFinal.List>

    @{
        ViewBag.Title = "Index";
    }

   
    <body>
        <a href="/Lists/Create" class="btn btn-primary" style="float:right">Nueva Lista</a>
        <div class="row">
            <div class="col-lg-12">
                <table class="table" style="width:100%">
                    <thead>
                        <tr>
                            <th>
                                UserID
                            </th>
                            <th>
                                Nombre de Lista
                            </th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var list in Model)
                        {
                            <tr>
                                <td>@list.UserID</td>
                                <td>@list.Name</td>

                                <td>
                                    <button type="button" class="btn btn-primary">Editar</button>
                                    <button type="button" class="btn btn-danger">Eliminar</button>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </body>

